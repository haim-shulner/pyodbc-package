{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Unit Test",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "env": {},
            "args": [
                "-s",
                "-x",
                "-ra",
                "--durations=10",
                "-vv",
                "${workspaceFolder}"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        }
    ]
}